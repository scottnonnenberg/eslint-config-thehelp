{
  "name": "@scottnonnenberg/eslint-config-thehelp",
  "version": "0.8.0",
  "description": "The way I write javascript.",
  "main": "index.js",
  "scripts": {
    "release": "standard-version",
    "test": "npm run lint && npm run validate && npm run find-missing && npm run find-deprecated",
    "lint": "eslint .",
    "find-missing": "npm run find-missing-core && npm run find-missing-default && npm run find-missing-react && npm run find-missing-test",
    "find-missing-core": "eslint-find-rules -u core.js",
    "find-missing-default": "eslint-find-rules -u test/indexCombined.js",
    "find-missing-react": "eslint-find-rules -u test/reactCombined.js",
    "find-missing-test": "eslint-find-rules -u test/testCombined.js",
    "find-deprecated": "npm run find-deprecated-core && npm run find-deprecated-default && npm run find-deprecated-react && npm run find-deprecated-test",
    "find-deprecated-core": "eslint-find-rules -d core.js",
    "find-deprecated-default": "eslint-find-rules -d test/indexCombined.js",
    "find-deprecated-react": "eslint-find-rules -d test/reactCombined.js",
    "find-deprecated-test": "eslint-find-rules -d test/testCombined.js",
    "validate": "node scripts/validate.js",
    "commitmsg": "validate-commit-msg"
  },
  "peerDependencies": {
    "@scottnonnenberg/eslint-plugin-thehelp": "^0.3.2",
    "eslint": "^8.2.0",
    "eslint-plugin-bdd": "^2.1.1",
    "eslint-plugin-chai-expect": "^3.0.0",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-immutable": "^1.0.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-no-loops": "^0.3.0",
    "eslint-plugin-react": "^7.27.0",
    "eslint-plugin-security": "^1.4.0"
  },
  "devDependencies": {
    "eslint-find-rules": "4.0.0",
    "husky": "7.0.4",
    "lodash": "4.17.21",
    "standard-version": "9.3.2",
    "validate-commit-msg": "2.14.0"
  },
  "config": {
    "validate-commit-msg": {
      "maxSubjectLength": 72
    }
  },
  "keywords": [
    "eslint",
    "eslintconfig",
    "thehelp",
    "functional"
  ],
  "author": "Scott Nonnenberg <scott@nonnenberg.com>",
  "license": "MIT",
  "homepage": "https://github.com/scottnonnenberg/eslint-config-thehelp",
  "repository": {
    "type": "git",
    "url": "git@github.com:scottnonnenberg/eslint-config-thehelp.git"
  }
}
